<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AM-requestFromClientLog">
    <DisplayName>AM-requestFromClientLog</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>slash</Name>
        <Template>\\</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>space</Name>
        <Value>\s</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>replaceQuote</Name>
        <Value>'</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>RequestFromClient</Name>
        <Template>{replaceAll(request.content,'"',replaceQuote)}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>RequestFromClient</Name>
        <Template>{replaceAll(RequestFromClient,slash,'')}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>RequestFromClient</Name>
        <Template>{replaceAll(RequestFromClient,space,'')}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>